{
    "name": "Python3 with GPU",
    "context": "..",
    "dockerFile": "Dockerfile",
  
    "remoteUser": "vscode",
  
    "runArgs": [
      "--gpus=all"
    ],
  
    "appPort": [
      "8888:8888", "5432:5432"
    ],
  
    // "postCreateCommand": "sudo service postgresql start",
  
    "postStartCommand": "jupyter lab --no-browser --ip=0.0.0.0 --port=8888 --allow-root --ServerApp.token=''"    
  
}
