{
    "name": "Python3, PostgreSQL with GPU",
    "context": "..",
    "dockerFile": "Dockerfile",
  
    "remoteUser": "vscode",
  
    "runArgs": [
      "--gpus=all"
    ],
  
    "appPort": [
      "8888", "5432"
    ],
  
    "postCreateCommand": "sudo service postgresql start",
  
    "postStartCommand": "jupyter lab --no-browser --ip=127.0.0.1 --port=8888 --allow-root --ServerApp.token=''"    
  
}
